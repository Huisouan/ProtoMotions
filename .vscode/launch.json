{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug train_agent.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/protomotions/train_agent.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "+exp=ase_mlp",
                "+robot=go2",
                "+simulator=genesis",
                "+experiment_name=go2_ase",
                "motion_file=data/motions/go2/go2_motions.yaml"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug eval_agent.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/protomotions/eval_agent.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "+robot=go2",
                "+simulator=genesis",
                "+checkpoint=results/go2_full_body_track/score_based.ckpt"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}